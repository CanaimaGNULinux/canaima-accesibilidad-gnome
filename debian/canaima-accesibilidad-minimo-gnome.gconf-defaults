# Se Activa Orca en el Gdm3 --Francisco Vasquez G. Esta Acción la realizara el instalador de canaima
#/desktop/gnome/applications/at/screen_reader_enabled true 
# -------------------------------------------------------
# Inicio Automatico de Gnome-orca --Francisco Vasquez G.(Con esto no levantaría la consola de congiguración de Orca)

/desktop/gnome/interface/accessibility true	
#--------------------------------------------------------

# Desactivar Valor del panel por parte del estilo visual de canaima --Francisco Vasquez G.

#/apps/metacity/global_keybindings/show_desktop disabled
#/apps/metacity/global_keybindings/panel_main_menu disabled
#--------------------------------------------------------

#---------------preferencias de Acceso Universal--------- 
#/desktop/gnome/accessibility/keyboard/enable true

#----------------Inicio de Orca combinación Super+o------  
#/apps/metacity/keybinding_commands/command_1 "orca -d main-window"

#/apps/metacity/global_keybindings/run_command_1 "<Super>o"

#----------------Inicio de Terminal gnome----------------

#/apps/metacity/keybinding_commands/command_2 "gnome-terminal"
	
#/apps/metacity/global_keybindings/run_command_2 "<Super>t"

#---------------Inicio de Libreoffice writer-------------	
#/apps/metacity/keybinding_commands/command_3 "libreoffice --writer"

#/apps/metacity/global_keybindings/run_command_3 "<Super>w"

#---------------Inicio de Cunaguaro----------------------

#/apps/metacity/keybinding_commands/command_4 "cunaguaro"

#/apps/metacity/global_keybindings/run_command_4 "<Super>n"

#---------------Inicio de Nautilus----------------------
#/apps/metacity/keybinding_commands/command_5 "nautilus"

#/apps/metacity/global_keybindings/run_command_5 "<Super>h"

#---------------Inicio de Impress-----------------------
#/apps/metacity/keybinding_commands/command_6 "libreoffice --impress"
	
#/apps/metacity/global_keybindings/run_command_6 "<Super>i"

#---------------Inicio de pidgin------------------------
#/apps/metacity/keybinding_commands/command_7 "pidgin"

#/apps/metacity/global_keybindings/run_command_7 "<Super>p"

